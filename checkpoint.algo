VAR 
i,j,k,sumv:integer:=0;
st:STRING;
BEGIN
i:=0;
read(st);
WHILE(st[i] <> ".") DO
    i:=i+1;
END_WHILE
    sumv:=0;
    j:=0;
    k:=0;
    
FOR j FROM 0 TO i DO
IF(st[j]==" ") THEN
k:=k+1;
END_IF
IF(Majus(st[j])=="A" or Majus(st[j])=="E" or Majus(st[j])=="U" or Majus(st[j])=="I" or Majus(st[j])=="O" ) THEN
sumv:=sumv+1;
END_IF
END_FOR
write("the number of word is",k+1,"the number of carecter is",i+1,"the number of vowls is",sumv);
END





